   "source": [
    "## Grouping ages into buckets\n",
    "\n",
    "def group_age(age):\n",
    "    if age < 10: \n",
    "        return \"<10\"\n",
    "    elif age < 20: \n",
    "        return \"10-19\"\n",
    "    elif age < 30: \n",
    "        return \"20-29\"\n",
    "    elif age < 40: \n",
    "        return \"30-39\"\n",
    "    elif age < 50: \n",
    "        return \"40-49\"\n",
    "    elif age < 60: \n",
    "        return \"50-59\"\n",
    "    else: \n",
    "        return \"60+\"\n",
    "\n",
    "titanic_df[\"Age_Group\"] = titanic_df[\"Age\"].apply(group_age)"
