    "\n",
    "fig, axes = plt.subplots(nrows=2, ncols=2)\n",
    "fig.set_figheight(12)\n",
    "fig.set_figwidth(16)\n",
    "\n",
    "sex_df = titanic_df.groupby(['Sex','Survived']).PassengerId.count()\n",
    "sex_df.unstack().plot(kind='bar',stacked=True,color=['red','blue'],ax=axes[0,0])\n",
    "\n",
    "age_group_df = titanic_df.groupby(['Age_Group','Survived']).PassengerId.count() \n",
    "age_group_df.unstack().plot(kind='bar',stacked=True,color=['red','blue'],ax=axes[0,1])\n",
    "\n",
